{
  "name": "hik-cloud-agentX",
  "version": "1.0.4",
  "description": "AI Assistant Component Library for Vue 2",
  "keywords": [
    "vue",
    "vue2",
    "ai",
    "chat",
    "component-library"
  ],
  "author": "",
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:watch": "vite build --watch",
    "preview": "vite preview",
    "prepublishOnly": "npm run build",
    "version:patch": "npm version patch",
    "version:minor": "npm version minor",
    "version:major": "npm version major",
    "release:patch": "npm run version:patch && npm run publish:latest",
    "release:minor": "npm run version:minor && npm run publish:latest",
    "release:major": "npm run version:major && npm run publish:latest",
    "release:beta": "npm version prerelease --preid=beta && npm publish --tag beta --access public",
    "publish:latest": "npm publish --access public",
    "publish:beta": "npm publish --tag beta --access public"
  },
  "main": "lib/hik-cloud-agentX.umd.js",
  "module": "lib/hik-cloud-agentX.es.js",
  "style": "lib/style.css",
  "sass": "src/style/index.scss",
  "files": [
    "lib",
    "src/style"
  ],
  "exports": {
    ".": {
      "import": "./lib/hik-cloud-agentX.es.js",
      "require": "./lib/hik-cloud-agentX.umd.js",
      "style": "./lib/style.css"
    },
    "./style": {
      "import": "./src/style/index.scss",
      "require": "./src/style/index.scss"
    }
  },
  "peerDependencies": {
    "vue": "^2.7.0"
  },
  "dependencies": {
    "@ctrl/tinycolor": "^4.2.0",
    "@microsoft/fetch-event-source": "^2.0.1",
    "ali-oss": "^6.23.0",
    "axios": "^1.6.8",
    "core-js": "^3.37.1",
    "crypto-js": "^4.2.0",
    "deepmerge": "^4.3.1",
    "echarts": "^5.5.1",
    "html2canvas": "^1.4.1",
    "js-base64": "^3.7.7",
    "jsrsasign": "10.8.6",
    "lodash": "^4.17.21",
    "marked": "^17.0.1",
    "moment": "^2.30.1",
    "node-uuid": "^1.4.8",
    "pako": "^2.1.0",
    "popper.js": "^1.16.1",
    "qrcode": "^1.5.4",
    "tencentcloud-speech-sdk-js": "^1.0.0",
    "typed.js": "^2.1.0",
    "underscore": "^1.13.7",
    "uuid": "^9.0.1",
    "video.js": "^8.9.0",
    "vue": "^2.7.14",
    "vuedraggable": "^2.24.3",
    "vuex": "^3.6.2"
  },
  "devDependencies": {
    "@vitejs/plugin-vue2": "^2.3.1",
